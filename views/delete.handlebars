
  {{>basic-header}}

<main>
    <div class = 'basic-message'>
        <img src = 'https://d30y9cdsu7xlg0.cloudfront.net/png/3058-200.png' alt="Trash">
        <h3>Delete an old color palette</h3>
        <h5>Select an option below:</h5>
        <div class='select-container'>
            <select id = 'delete-canvas'>
                <option value="">Select a color set</option>
                {{#each color}}
                    <option value='{{@key}}'>{{title}}</option>
                {{/each}}
            </select>
        </div>
    </div>
</main>
